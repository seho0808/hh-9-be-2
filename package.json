{
  "name": "nestjs-test-containers",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/e2e/jest-e2e.json",
    "test:integration": "jest --config ./test/integration/jest-integration.json",
    "k6:coupon": "k6 run scripts/k6-lock-test/k6-coupon-load-test.js",
    "k6:coupon:database": "BASE_URL=http://[::1]:3000 LOCK_STRATEGY=database COUPON_ID=test-coupon-1 k6 run scripts/k6-lock-test/k6-coupon-load-test.js",
    "k6:coupon:spinlock": "BASE_URL=http://[::1]:3000 LOCK_STRATEGY=spinlock COUPON_ID=test-coupon-1 k6 run scripts/k6-lock-test/k6-coupon-load-test.js",
    "k6:coupon:pubsub": "BASE_URL=http://[::1]:3000 LOCK_STRATEGY=pubsub COUPON_ID=test-coupon-1 k6 run scripts/k6-lock-test/k6-coupon-load-test.js",
    "k6:coupon:queue": "BASE_URL=http://[::1]:3000 LOCK_STRATEGY=queue COUPON_ID=test-coupon-1 k6 run scripts/k6-lock-test/k6-coupon-load-test.js",
    "k6:coupon:fencing": "BASE_URL=http://[::1]:3000 LOCK_STRATEGY=fencing COUPON_ID=test-coupon-1 k6 run scripts/k6-lock-test/k6-coupon-load-test.js",
    "k6:coupon:redlock": "BASE_URL=http://[::1]:3000 LOCK_STRATEGY=redlock COUPON_ID=test-coupon-1 k6 run scripts/k6-lock-test/k6-coupon-load-test.js",
    "seed:coupon": "ts-node -r tsconfig-paths/register scripts/k6-lock-test/seed-test-coupon.ts",
    "cleanup:test-data": "ts-node -r tsconfig-paths/register scripts/k6-lock-test/cleanup-test-data.ts",
    "cleanup:test-users": "ts-node -r tsconfig-paths/register scripts/k6-lock-test/cleanup-test-data.ts users",
    "reset:coupon-counts": "ts-node -r tsconfig-paths/register scripts/k6-lock-test/cleanup-test-data.ts reset",
    "k6:cache:popular-products": "BASE_URL=http://[::1]:3000 CACHE_ENABLED=true k6 run scripts/k6-cache-test/k6-popular-products-cache-test.js",
    "k6:cache:popular-products:disabled": "BASE_URL=http://[::1]:3000 CACHE_ENABLED=false k6 run scripts/k6-cache-test/k6-popular-products-cache-test.js",
    "k6:cache:user-orders": "BASE_URL=http://[::1]:3000 CACHE_ENABLED=true k6 run scripts/k6-cache-test/k6-user-orders-cache-test.js",
    "k6:cache:user-orders:disabled": "BASE_URL=http://[::1]:3000 CACHE_ENABLED=false k6 run scripts/k6-cache-test/k6-user-orders-cache-test.js",
    "k6:cache:product-details": "BASE_URL=http://[::1]:3000 CACHE_ENABLED=true k6 run scripts/k6-cache-test/k6-product-details-cache-test.js",
    "k6:cache:product-details:disabled": "BASE_URL=http://[::1]:3000 CACHE_ENABLED=false k6 run scripts/k6-cache-test/k6-product-details-cache-test.js",
    "seed:cache-test-data": "ts-node -r tsconfig-paths/register scripts/k6-cache-test/seed-cache-test-data.ts",
    "cleanup:cache-test-data": "ts-node -r tsconfig-paths/register scripts/k6-cache-test/cleanup-cache-test-data.ts",
    "cleanup:cache-test-data:db": "ts-node -r tsconfig-paths/register scripts/k6-cache-test/cleanup-cache-test-data.ts db",
    "cleanup:cache-test-data:cache": "ts-node -r tsconfig-paths/register scripts/k6-cache-test/cleanup-cache-test-data.ts cache",
    "flush:cache": "ts-node -r tsconfig-paths/register scripts/k6-cache-test/cleanup-cache-test-data.ts flush",
    "cache:stats": "ts-node -r tsconfig-paths/register scripts/k6-cache-test/cleanup-cache-test-data.ts stats"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/typeorm": "^10.0.2",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "ioredis": "^5.7.0",
    "mysql2": "^3.9.2",
    "redlock": "5.0.0-beta.2",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "typeorm-transactional": "^0.5.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@testcontainers/mysql": "^10.7.2",
    "@testcontainers/redis": "^11.5.1",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "testcontainers": "^10.7.2",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    }
  }
}
